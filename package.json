{
  "name": "@lesleh/gpt-prompts",
  "version": "0.2.0",
  "description": "Collection of prompts to use with GPT Chat API",
  "keywords": [
    "openai",
    "gpt",
    "gpt-3",
    "chatbot",
    "prompts"
  ],
  "author": {
    "name": "Leslie Hoare",
    "email": "iam@lesleh.co.uk"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/gpt-prompts.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "del dist",
    "pristine": "git clean -xdf && git reset --hard",
    "build": "pnpm clean && concurrently -g npm:build:*",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "start": "nodemon --exec ts-node src/index.ts",
    "prepack": "pnpm build",
    "test": "vitest",
    "check-types": "tsc --noEmit",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitest/coverage-c8": "^0.29.8",
    "concurrently": "^8.0.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.37.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "openai": "^3.2.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
